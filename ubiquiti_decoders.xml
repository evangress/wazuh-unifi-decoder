<decoder name="ubiquiti">
	<prematch>Ubiquiti</prematch>
</decoder>

        <!-- CEF header fields -->
<!-- Standard Wazuh Fields: vendor, product -->
<decoder name="ubiquiti">
	<parent>ubiquiti</parent>
	<regex type="pcre2">(CEF):([0-9]+)\|(Ubiquiti)\|(UniFi\s+\w+)\|([^|]*)\|([^|]*)\|([^|]*)\|([0-9])|</regex>
	<order>cef.tag, cef.version, vendor, product, device.version, event.id, event.name, event.severity</order>
</decoder>

        <!-- UniFi namespaced keys: capture any key starting with UNIFI followed by alphanumerics -->
<decoder name="ubiquiti">
	<parent>ubiquiti</parent>
	<regex type="pcre2">UNIFIcategory=([^\s].*?)\s*(?=[A-Za-z0-9_.:-]+=|$)</regex>
	<order>unifi_category</order>
</decoder>

